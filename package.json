    "rollup": "^2.64.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "node-html-parser": "^5.2.0"
  }
